@model IEnumerable<HospitalApplication.Models.DoctorDto>


@{
    ViewBag.Title = "List";
}

<h2>List of Doctors</h2>

<a href="/Doctor/New">Add New Doctor</a>

@foreach (var doctor in Model)
{

    <h2><a href="/Doctor/Details/@doctor.DoctorId">@doctor.DoctorFirstName @doctor.DoctorLastName</a></h2>
    <p>@doctor.Department</p>

}